{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@app/*": ["app/*"],
      "@assets/*": ["assets/*"],
      "@entities/*": ["entities/*"],
      "@features/*": ["features/*"],
      "@hooks/*": ["hooks/*"],
      "@shared/*": ["shared/*"],
      "@pages/*": ["pages/*"],
      "@routeTree": ["routeTree.gen.ts"]
    },
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src"]
}
